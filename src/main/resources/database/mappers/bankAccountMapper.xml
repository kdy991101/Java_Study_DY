<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
    
<mapper namespace="com.iu.start.bankAccount.BankAccountDAO">

	<insert id="add" parameterType="com.iu.start.bankAccount.BankAccountDTO">
		INSERT INTO BANKACCOUNT VALUES (ACCOUNT_SEQ.NAXTVAL,#{username},#{bookNum},SYSDATE)
		</insert>
		
		<select id="getlist" parameterType="String" resultType="BankAccountDTO">
		SELECT * FROM BANKACCOUNT WHERE BOOKNUM = #{bookNum}				
		</select>
		
	</mapper> 